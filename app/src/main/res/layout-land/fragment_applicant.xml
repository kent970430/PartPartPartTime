<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/frameLayout2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ui.applicant_profile.ApplicantFragment" >

        <EditText
            android:id="@+id/textEmaill"
            android:layout_width="146dp"
            android:layout_height="41dp"
            android:layout_marginTop="15dp"
            android:enabled="false"
            android:hint="@string/email"
            android:inputType="textEmailAddress"
            android:textSize="17dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.088"
            app:layout_constraintStart_toEndOf="@+id/imageViewPreview2"
            app:layout_constraintTop_toBottomOf="@+id/textName" />

        <Button
            android:id="@+id/buttonSave"
            android:layout_width="150dp"
            android:layout_height="35dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="20dp"
            android:background="@drawable/homebutton"
            android:padding="10dp"
            android:text="@string/save"
            android:textColor="#000000"
            app:layout_constraintEnd_toStartOf="@+id/fab"
            app:layout_constraintHorizontal_bias="0.147"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/buttonEdit" />

        <Button
            android:id="@+id/buttonEdit"
            android:layout_width="150dp"
            android:layout_height="35dp"
            android:layout_marginTop="12dp"
            android:background="@drawable/homebutton"
            android:padding="10dp"
            android:text="@string/edit"
            android:textColor="#000000"
            app:layout_constraintEnd_toStartOf="@+id/textDescription3"
            app:layout_constraintHorizontal_bias="0.692"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/chooseimage" />

        <EditText
            android:id="@+id/textDescription2"
            android:layout_width="289dp"
            android:layout_height="36dp"
            android:layout_marginTop="15dp"
            android:enabled="false"
            android:hint="@string/skill"
            android:padding="10dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.574"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textDescription1" />


        <EditText
            android:id="@+id/textDescription3"
            android:layout_width="289dp"
            android:layout_height="36dp"
            android:layout_marginTop="12dp"
            android:enabled="false"
            android:hint="@string/education"
            android:padding="10dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.574"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textDescription2" />

        <EditText
            android:id="@+id/textDescription1"
            android:layout_width="289dp"
            android:layout_height="36dp"
            android:layout_marginTop ="15dp"
            android:enabled="false"
            android:hint="@string/experience"
            android:padding="10dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.574"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textEmaill" />

        <EditText
            android:id="@+id/textDescription"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:enabled="false"
            android:hint="@string/contact"
            android:textSize="17dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.372"
            app:layout_constraintStart_toEndOf="@+id/textName"
            app:layout_constraintTop_toTopOf="@+id/textName" />

        <EditText
            android:id="@+id/textName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="32dp"
            android:layout_marginTop="36dp"
            android:enabled="false"
            android:text="@string/nameee"
            android:textSize="17dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/imageViewPreview2"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/imageViewPreview2"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="36dp"
            android:layout_marginRight="8dp"
            app:layout_constraintHorizontal_bias="0.113"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/no_image" />

        <EditText
            android:id="@+id/textRating"
            android:layout_width="289dp"
            android:layout_height="36dp"
            android:layout_marginTop="16dp"
            android:enabled="false"
            android:gravity="center"
            android:padding="10dp"
            android:text="@string/rating"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.574"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textDescription3" />

        <Button
            android:id="@+id/buttonLogOut"
            android:layout_width="150dp"
            android:layout_height="35dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/homebutton"
            android:padding="10dp"
            android:text="@string/logout"
            android:textColor="#000000"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.123"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/buttonSave" />

        <Button
            android:id="@+id/chooseimage"
            android:layout_width="150dp"
            android:layout_height="35dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/homebutton"
            android:padding="10dp"
            android:text="@string/chooseimage"
            android:textColor="#000000"
            android:visibility="invisible"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.123"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageViewPreview2" />

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/fab"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom|end"
            android:layout_margin="@dimen/fab_margin"
            android:visibility="invisible"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:srcCompat="@drawable/ic_add_whilte_24dp" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</layout>